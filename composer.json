{
  "name": "vesperphp/project",
  "type": "project",
  "description": "Vesper PHP, build fast.",
  "keywords": ["framework", "vesperphp", "php"],
  "license": "MIT",
  
  "require": {
      "vesperphp/elemental": "0.*"
  },

  "autoload": {
    "classmap": [
      "app/models", 
      "app/controllers", 
      "app/filters",
      "vendor/vesperphp"
    ]
  },

  "config": {
    "optimize-autoloader": true
  }

}
